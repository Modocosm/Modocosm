---
const data = Astro.props.data
---

<title>{data.title}</title>

<link rel="icon"            type="image/x-icon"     href={data.favicon.url} />
<link rel="canonical"       href={data.rootURL}                             />
<meta charset="UTF-8"                                                       />
<meta name="theme-color"    content={data.themeColor}                       />
<meta name="viewport"       content="width=device-width, initial-scale=1"   />
<meta name="description"    content={data.description}                      />
<meta name="keywords"       content={data.keywords}                         />
<meta name="author"         content={data.author}                           />
<meta name="subject"        content={data.subject}                          />
<meta name="copyright"      content={data.copywrite}                        />
<meta name="Classification" content={data.classification}                   />